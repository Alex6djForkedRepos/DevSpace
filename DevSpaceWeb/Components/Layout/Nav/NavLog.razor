<div class="nav-list nav-list-sub nav-errors">
    <div class="nav-list-header">
        <span class="iconify" data-icon="mdi:warning"></span>
        <h5 style="color: var(--rz-menu-top-item-color) !important; text-decoration: none !important; height: 40px;">@Session.Selected.Log.Name</h5>
    </div>
    <RadzenPanelMenuItem Path="@("/teams/" + Session.Selected.Team.VanityUrl + "/logs/" + Session.Selected.Team.Log.VanityUrl + "/home")" Match="NavLinkMatch.All">
        <Template>
            <span class="iconify" data-icon="mdi:home"></span>
            <p>Home</p>
        </Template>
    </RadzenPanelMenuItem>
