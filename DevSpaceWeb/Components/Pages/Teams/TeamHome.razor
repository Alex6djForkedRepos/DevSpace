
<div class="dash-box dash-box-content">
    <div class="dash-box-header">
        <div class="dash-box-icon"><span class="iconify" data-icon="mdi:account-supervisor-circle"></span></div>
        <RadzenText TextStyle="TextStyle.H6">Team</RadzenText>
    </div>
    <RadzenBreadCrumb class="mt-2">
        <RadzenBreadCrumbItem Path="@("/teams")" Text="Teams" />
        <RadzenBreadCrumbItem Text="@Session.Selected.Team.Data.Name" />
    </RadzenBreadCrumb>
    <br />
</div>

<TeamSettings IsHome="true"></TeamSettings>



@code {
    [CascadingParameter]
    private SessionProvider Session { get; set; }

    public TeamData Team => Session.Selected.Team.Data;
    public TeamMemberData Member => Session.Selected.Member;

}
